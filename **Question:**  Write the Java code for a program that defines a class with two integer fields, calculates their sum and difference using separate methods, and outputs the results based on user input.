import java.io.*;
import java.util.*;

class Add
{
	public static void main(String[] args)
	{
		sumoop s = new sumoop();
		s.cal();
	}
}
class sumoop
{
	int x=10,y=5;
	void cal()
	{
		System.out.println(x+y);
	}
}
//output
//Enter the first number (a): 8
//Enter the second number (b): 3
//Sum of a and b: 11
//Difference between a and b: 5
