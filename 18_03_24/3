import java.io.*;
import java.util.*;

class factors
{
	public static void main(String[] args)
	{
		int n,c=0;
		Scanner S = new Scanner(System.in);
		System.out.println("Enter the number");
		n = S.nextInt();
		for(int i=1;i<=n;i++)
		{
			if(n%i==0)
			{
				c++;
			}
		}
		System.out.println(c);
	}
}
